from util import if_elsefrom Compiler import types, libraryprogram.bit_length = 32def compute_intersection():    a = Array(100, sint)    @for_range(100)    def _(i):        a[i] = i            b = a.get_vector(base =1, size=99)    c = a.get_vector(base=0, size=99)                d = Array(99, sint)    @for_range_opt(99)    def _(i):        d[i] = a[i] + a[i+1]    @for_range_opt(99)    def _(i):        a[i] = d[i]        return a        def compute_intersection_multi():    a = Array(100, sint)    d = Array(99, sint)    @for_range(100)    def _(i):        a[i] = i            b = a.get_vector(base = 1, size = 99)    c = a.get_vector(size = 99)                d = c + b # adds b and c elementwise - an implicit loop    a.assign_vector(d, base = 0)#    d.assign_vector(a, base = 0, size = 99) # Replaces the first 99 elements of a by those of c        return dcompute_intersection_multi()